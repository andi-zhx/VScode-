                    "ReadOnly": {
                      "description": "Specifies whether the table is read-only.",
                      "type": "boolean",
                      "enum": [
                        true,
                        false
                      ]
                    },
                    "SQL": {
                      "description": "SQL SELECT statement.",
                      "type": "string"
                    },
                    "Fields": {
                      "description": "Fields used in the current main table.",
                      "type": "array",
                      "items": [
                        {
                          "type": "object",
                          "patternProperties": {
                            "^[a-z][a-z0-9/-_]*$": {
                              "additionalProperties": false,
                              "required": [
                                "type",
                                "title",
                                "widget",
                                "show",
                                "disable",
                                "enumdata"
                              ],
                              "properties": {
                                "type": {
                                  "description": "Data type of this field.",
                                  "type": "string"
                                },
                                "allowNull": {
                                  "description": "Allow null of this field?",
                                  "type": "boolean"
                                },
                                "primaryKey": {
                                  "description": "Is this field the primary key?",
                                  "type": "boolean"
                                },
                                "comment": {
                                  "description": "Comment of this field",
                                  "type": "string"
                                },
                                "title": {
                                  "description": "Title of this field",
                                  "type": "string"
                                },
                                "autoIncrement": {
                                  "description": "Is this field auto incremented?",
                                  "type": "boolean"
                                },
                                "unique": {
                                  "description": "Is this field auto unique?",
                                  "type": "boolean"
                                },
                                "defaultValue": {
                                  "description": "Default value of this field.",
                                  "type": "string"
                                },
                                "widget": {
                                  "description": "Widget for this field.",
                                  "type": "string",
                                  "enum": [
                                    "Textbox",
                                    "Text Area",
                                    "Combobox",
                                    "Checkbox",
                                    "Radio",
                                    "Datepicker"
                                  ]
                                },
                                "show": {
                                  "description": "Show/hide this widget.",
                                  "type": "boolean"
                                },
                                "disable": {
                                  "description": "Disable this widget.",
                                  "type": "boolean"
                                },
                                "enumdata": {
                                  "description": "Enumeration data this widget.",
                                  "type": "array"
                                }
                              },
                              "if": {
                                "properties": {
                                  "widget": {
                                    "enum": [
                                      "Checkbox"
                                    ]
                                  }
                                }
                              },
                              "then": {
                                "properties": {
                                  "type": {
                                    "const":["DataTypes.BOOLEAN"] 
                                  }
                                }
                              }
                            }
                          }
                        }
                      ]
                    },
                    "FieldsNotNull": {
                      "description": "Specify not-null fields in the table.",
                      "type": "array"
                    },
