                "Button": {
                  "patternProperties": {
                    "^[a-z][a-z0-9/-]*$": {
                      "additionalProperties": false,
                      "required": [
                        "Label",
                        "Function",
                        "Show",
                        "Shape",
                        "Size",
                        "Style"
                      ],
                      "properties": {
                        "Label": {
                          "description": "Label on this button.",
                          "type": "string"
                        },
                        "Function": {
                          "description": "Select function of this button.",
                          "type": "string",
                          "enum": [
                            "Edit",
                            "Delete",
                            "Export",
                            "Link",
                            "Custom"
                          ]
                        },
                        "Show": {
                          "description": "Show/hide this button.",
                          "type": "boolean",
                          "default": true
                        },
                        "Shape": {
                          "description": "Shape of this button.",
                          "type": "string",
                          "enum": [
                            "Rectangle",
                            "Semi-round",
                            "Round"
                          ],
                          "default": "Rectangle"
                        },
                        "Size": {
                          "description": "Size of this button.",
                          "type": "string",
                          "enum": [
                            "Tiny",
                            "Small",
                            "Medium",
                            "Large",
                            "Giant"
                          ],
                          "default": "Medium"
                        },
                        "Style": {
                          "description": "Style of this button.",
                          "type": "string",
                          "enum": [
                            "Hero",
                            "Default",
                            "Outline",
                            "Ghost"
                          ],
                          "default": "Hero"
                        },
                        "Implication": {
                          "description": "The Implication of this button - represented by color.",
                          "type": "string",
                          "enum": [
                            "Primary",
                            "Success",
                            "Info",
                            "Warning",
                            "Danger",
                            "Disabled"
                          ]
                        },
                        "TargetFunction": {
                          "description": "Target function of this link.",
                          "type": "string"
                        },
                        "QueryVar": {
                          "description": "Query variable of this function.",
                          "type": "string"
                        },
                        "TargetQueryVar": {
                          "description": "Query variable of the target function.",
                          "type": "string"
                        }
                      },
                      "if": {
                        "properties": {
                          "Function": {
                            "enum": [
                              "Link"
                            ]
                          }
                        }
                      },
                      "then": {
                        "required": [
                          "TargetFunction",
                          "QueryVar",
                          "TargetQueryVar"
                        ]
                      }
                    }
                  }
                },
