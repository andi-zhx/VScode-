                "MainTable": {
                  "required": [
                    "Name",
                    "ReadOnly",
                    "SQL",
                    "Fields",
                    "FieldsNotNull",
                    "CalculationFormula"
                  ],
                  "properties": {
                    "Name": {
                      "description": "Table name of main table.",
                      "type": "string",
                      "enum": [
                        "image",
                        "restore_password",
                        "role",
                        "settings",
                        "user",
                        "user_roles"
